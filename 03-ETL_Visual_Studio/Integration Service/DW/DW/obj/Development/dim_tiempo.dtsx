      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{7C8503DE-8F28-4533-8A4B-EB9E07AF8970}"
          SQLTask:SqlStatementSource="-- Configurar idioma para que los nombres salgan en Español&#xA;SET LANGUAGE Spanish;&#xA;&#xA;-- Limpiar la tabla por si acaso (para no duplicar si corres varias veces)&#xA;DELETE FROM DimTiempo;&#xA;&#xA;-- Variables para el rango de fechas (Ajústalo si necesitas más años)&#xA;DECLARE @FechaActual DATE = '2020-01-01';&#xA;DECLARE @FechaFin    DATE = '2030-12-31';&#xA;&#xA;WHILE @FechaActual &lt;= @FechaFin&#xA;BEGIN&#xA;    INSERT INTO DimTiempo (&#xA;        Fecha,&#xA;        Anio,&#xA;        Mes,&#xA;        NombreMes,&#xA;        Trimestre,&#xA;        Semestre,&#xA;        Dia,&#xA;        DiaSemana,&#xA;        NombreDia,&#xA;        EsFinDeSemana&#xA;    )&#xA;    VALUES (&#xA;        @FechaActual,&#xA;        YEAR(@FechaActual),&#xA;        MONTH(@FechaActual),&#xA;        DATENAME(MONTH, @FechaActual),&#xA;        DATEPART(QUARTER, @FechaActual),&#xA;        CASE WHEN MONTH(@FechaActual) &lt;= 6 THEN 1 ELSE 2 END,&#xA;        DAY(@FechaActual),&#xA;        DATEPART(WEEKDAY, @FechaActual),&#xA;        DATENAME(WEEKDAY, @FechaActual),&#xA;        -- Detectar fin de semana (Sábado o Domingo)&#xA;        CASE WHEN DATEPART(WEEKDAY, @FechaActual) IN (1, 7) THEN 1 ELSE 0 END &#xA;        -- OJO: Si tu SQL está configurado diferente y el 1 no es Domingo, esto podría variar, &#xA;        -- pero para el examen suele funcionar estándar.&#xA;    );&#xA;&#xA;    -- Avanzar un día&#xA;    SET @FechaActual = DATEADD(DAY, 1, @FechaActual);&#xA;END" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
  </DTS:Executables>
